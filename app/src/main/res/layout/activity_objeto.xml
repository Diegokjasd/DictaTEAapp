<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Objeto">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:id="@+id/linearLayout"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="620dp"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/yoquieroquieroobj"
                    android:layout_width="375dp"
                    android:layout_height="38dp"
                    android:layout_gravity="center_horizontal|center"
                    android:layout_marginLeft="-3dp"
                    android:layout_marginTop="10dp"
                    android:fontFamily="sans-serif-black"
                    android:gravity="center"
                    android:text="Yo quiero ________"
                    android:textColor="@color/_bg__yoquiero_color"
                    android:textSize="20sp" />


                <ImageView
                    android:id="@+id/quieroviewobj"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:contentDescription="quieroview"
                    android:foreground="?android:attr/selectableItemBackground"
                    android:src="@drawable/yo_quiero" />

                <LinearLayout
                    android:id="@+id/linearlayout2"
                    android:layout_width="match_parent"
                    android:layout_height="457dp"
                    android:orientation="vertical"
                    android:gravity="center">

                    <TableLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center">

                        <TableRow
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:gravity="center">

                            <TextView
                                android:id="@+id/lapiztx"
                                android:layout_width="120dp"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="15dp"
                                android:layout_marginTop="1dp"
                                android:fontFamily="sans-serif-black"
                                android:gravity="center"
                                android:text="el lapiz"
                                android:textColor="@color/_bg__yoquiero_color"
                                android:textSize="20sp" />

                            <TextView
                                android:id="@+id/pelotatx"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="15dp"
                                android:layout_marginTop="1dp"
                                android:fontFamily="sans-serif-black"
                                android:gravity="center"
                                android:text="@string/pelotaframetx_string"
                                android:textColor="@color/_bg__yoquiero_color"
                                android:textSize="20sp" />

                            <TextView
                                android:id="@+id/juguetetx"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="15dp"
                                android:layout_marginTop="1dp"
                                android:fontFamily="sans-serif-black"
                                android:gravity="center"
                                android:text="@string/jugueteframetx_string"
                                android:textColor="@color/_bg__yoquiero_color"
                                android:textSize="20sp" />
                        </TableRow>

                        <TableRow
                            android:layout_width="match_parent"
                            android:layout_height="64dp"
                            android:gravity="center">

                            <Button
                                android:id="@+id/lapiz"
                                android:layout_width="114dp"
                                android:layout_height="100dp"
                                android:layout_marginStart="15dp"
                                android:layout_marginTop="1dp"
                                android:background="@drawable/lapizframe"
                                android:contentDescription="@string/lapizframetx_string"
                                android:foreground="?android:attr/selectableItemBackground"
                                android:gravity="center"
                                android:onClick="MensajeLapiz"
                                app:backgroundTint="@null" />

                            <Button
                                android:id="@+id/pelota"
                                android:layout_width="114dp"
                                android:layout_height="100dp"
                                android:layout_marginStart="15dp"
                                android:layout_marginTop="1dp"
                                android:background="@drawable/pelotaframe"
                                android:contentDescription="pelota"
                                android:foreground="?android:attr/selectableItemBackground"
                                android:gravity="center"
                                android:onClick="MensajePelota"
                                app:backgroundTint="@null" />

                            <Button
                                android:id="@+id/juguete"
                                android:layout_width="114dp"
                                android:layout_height="100dp"
                                android:layout_marginStart="15dp"
                                android:layout_marginTop="1dp"
                                android:background="@drawable/jugueteframe"
                                android:contentDescription="juguete"
                                android:foreground="?android:attr/selectableItemBackground"
                                android:gravity="center"
                                android:onClick="MensajeJuguete"
                                app:backgroundTint="@null" />

                        </TableRow>

                        <TableRow
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:gravity="center">

                            <TextView
                                android:id="@+id/librotx"
                                android:layout_width="103dp"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="15dp"
                                android:layout_marginTop="1dp"
                                android:fontFamily="sans-serif-black"
                                android:gravity="center"
                                android:text="@string/libroframetx_string"
                                android:textColor="@color/_bg__yoquiero_color"
                                android:textSize="20sp" />

                            <TextView
                                android:id="@+id/peluchetx"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="15dp"
                                android:layout_marginTop="1dp"
                                android:fontFamily="sans-serif-black"
                                android:gravity="center"
                                android:text="@string/pelucheframetx_string"
                                android:textColor="@color/_bg__yoquiero_color"
                                android:textSize="20sp" />

                            <TextView
                                android:id="@+id/controltx"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="15dp"
                                android:layout_marginTop="1dp"
                                android:fontFamily="sans-serif-black"
                                android:gravity="center"
                                android:text="@string/controlframetx_string"
                                android:textColor="@color/_bg__yoquiero_color"
                                android:textSize="20sp" />
                        </TableRow>

                        <TableRow
                            android:layout_width="match_parent"
                            android:layout_height="205dp"
                            android:gravity="center">

                            <Button
                                android:id="@+id/libro"
                                android:layout_width="114dp"
                                android:layout_height="100dp"
                                android:layout_marginStart="15dp"
                                android:layout_marginTop="1dp"
                                android:background="@drawable/libroframe"
                                android:contentDescription="libro"
                                android:foreground="?android:attr/selectableItemBackground"
                                android:gravity="center"
                                android:onClick="MensajeLibro"
                                app:backgroundTint="@null" />

                            <Button
                                android:id="@+id/peluche"
                                android:layout_width="114dp"
                                android:layout_height="100dp"
                                android:layout_marginStart="15dp"
                                android:layout_marginTop="1dp"
                                android:background="@drawable/pelucheframe"
                                android:contentDescription="peluche"
                                android:foreground="?android:attr/selectableItemBackground"
                                android:gravity="center"
                                android:onClick="MensajePeluche"
                                app:backgroundTint="@null" />

                            <Button
                                android:id="@+id/control"
                                android:layout_width="114dp"
                                android:layout_height="100dp"
                                android:layout_marginStart="15dp"
                                android:layout_marginTop="1dp"
                                android:background="@drawable/controlframe"
                                android:contentDescription="control"
                                android:foreground="?android:attr/selectableItemBackground"
                                android:gravity="center"
                                android:onClick="MensajeControl"
                                app:backgroundTint="@null" />
                        </TableRow>

                        <TableRow
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:gravity="center">

                            <TextView
                                android:id="@+id/botellatx"
                                android:layout_width="21dp"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="15dp"
                                android:layout_marginTop="1dp"
                                android:fontFamily="sans-serif-black"
                                android:gravity="center"
                                android:text="@string/botellaframetx_string"
                                android:textColor="@color/_bg__yoquiero_color"
                                android:textSize="20sp" />

                            <TextView
                                android:id="@+id/relojtx"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="15dp"
                                android:layout_marginTop="1dp"
                                android:fontFamily="sans-serif-black"
                                android:gravity="center"
                                android:text="@string/relojframetx_string"
                                android:textColor="@color/_bg__yoquiero_color"
                                android:textSize="20sp" />

                        </TableRow>

                        <TableRow
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:gravity="center">

                            <Button
                                android:id="@+id/botella"
                                android:layout_width="114dp"
                                android:layout_height="100dp"
                                android:layout_marginStart="15dp"
                                android:layout_marginTop="1dp"
                                android:layout_marginBottom="15dp"
                                android:background="@drawable/beber"
                                android:contentDescription="botella"
                                android:foreground="?android:attr/selectableItemBackground"
                                android:gravity="center"
                                android:onClick="MensajeBotella"
                                app:backgroundTint="@null" />

                            <Button
                                android:id="@+id/reloj"
                                android:layout_width="114dp"
                                android:layout_height="100dp"
                                android:layout_marginStart="15dp"
                                android:layout_marginTop="1dp"
                                android:layout_marginBottom="15dp"
                                android:background="@drawable/relojframe"
                                android:contentDescription="reloj"
                                android:foreground="?android:attr/selectableItemBackground"
                                android:gravity="center"
                                android:onClick="MensajeReloj"
                                app:backgroundTint="@null" />
                        </TableRow>

                    </TableLayout>


                </LinearLayout>

            </LinearLayout>

        </LinearLayout>

        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:id="@+id/fbatras"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:backgroundTint="@color/barra_color"
            android:clickable="true"
            android:contentDescription="@string/floating"
            android:onClick="callAtras"
            app:backgroundTint="@color/barrahome2_color"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.076"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.97"
            app:srcCompat="@drawable/ic_baseline_subdirectory_arrow_left_24"
            tools:ignore="OnClick" />

        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:id="@+id/fbaHome"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:backgroundTint="@color/barra_color"
            android:clickable="true"
            android:contentDescription="@string/floating"
            android:onClick="callHome"
            app:backgroundTint="@color/barrahome2_color"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.95"
            app:layout_constraintStart_toEndOf="@+id/fbatras"
            app:layout_constraintTop_toTopOf="@+id/linearLayout"
            app:layout_constraintVertical_bias="0.97"
            app:srcCompat="@drawable/ic_home"
            tools:ignore="OnClick" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</RelativeLayout>